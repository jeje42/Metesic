{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "declaration": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "noImplicitAny": false,
    "noFallthroughCasesInSwitch": false,
    "noImplicitReturns": false
  },
  "exclude": [ ],
  "filesGlob": [
    "**/*.ts"
  ],
  "compileOnSave": false,
  "angularCompilerOptions": {
    "genDir": "aot",
    "skipMetadataEmit": true
  }
}
